<UserControl x:Class="FtpExplorerApp.FileEntityUserControll"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FtpExplorerApp"
             mc:Ignorable="d" 
             d:DesignHeight="32.773" d:DesignWidth="800">
    <Grid>
        <TextBlock x:Name="fileNameTextBlock" HorizontalAlignment="Left" TextWrapping="Wrap" Text="FileName" VerticalAlignment="Top" Height="33" Width="761" Margin="39,0,0,0"/>
        <Image x:Name="fileImage" HorizontalAlignment="Left" Height="33" VerticalAlignment="Top" Width="34"/>
    </Grid>
</UserControl>
